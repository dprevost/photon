#ifndef PSO_H_CPP
#define PSO_H_CPP

// --+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--

#include <photon/psoErrors.h>
#include <photon/psoCommon.h>
#include <photon/psoPhotonODBC.h>

#include <photon/Exception>

#include <photon/DataDefinition>
#include <photon/KeyDefinition>

#include <photon/Process>
#include <photon/Session>
#include <photon/Folder>
/*
 * Hash maps and fast maps implements the same data container but differs 
 * with respect to transactions. 
 * 
 * Hash maps are read-write and locks are used to insure data integrity.
 * Fast maps are read-only (with an update procedure using the class
 * FastMapEditor) and no locks are required.
 */

#include <photon/HashMap>
#include <photon/FastMap>
#include <photon/FastMapEditor>
#include <photon/Lifo>
#include <photon/Queue>

/*
 * The next classes are optional (helper classes).
 */
#include <photon/DataDefBuilderODBC>
#include <photon/DataDefBuilderUser>
#include <photon/KeyDefBuilderODBC>
#include <photon/KeyDefBuilderUser>

/**
 *
 * \namespace pso The default namespace for Photon (pso is the acronym of
 * photonsoftware.org, the Photon website).
 */
 
// --+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--

#endif // PSO_H_CPP

// --+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--+--

